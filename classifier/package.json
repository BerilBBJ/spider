{
  "name": "classifier",
  "version": "1.0.0",
  "description": "Classifies web content with provided labels. Uses semisupervised active learning in order to work better on small datasets.",
  "private": true,
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "start": "node --use_strict --max-old-space-size=16384 index.js",
    "runHost": "cp ../dataPreprocessing/models ./models && cp ../server/.env . && node --max-old-space-size=16384 index.js",
    "debugHost": "cp ../dataPreprocessing/models ./models && cp ../server/.env . && export DEBUG=true && node --inspect-brk=0.0.0.0:65101 --use_strict index.js",
    "debugVirt": "export DEBUG=true && node --inspect-brk=0.0.0.0:65001 --use_strict index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/decrypto-org/spider.git"
  },
  "keywords": [
    "classification",
    "content analysis",
    "bag of words",
    "svm"
  ],
  "author": "Roman Brunner (robrunne@student.ethz.ch)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/decrypto-org/spider/issues"
  },
  "homepage": "https://github.com/decrypto-org/spider#readme",
  "dependencies": {
    "command-line-args": "^5.0.2",
    "csvjson": "^5.0.0",
    "dotenv": "^6.0.0",
    "dotenv-expand": "^4.2.0",
    "mathjs": "^5.0.4",
    "pg": "^7.4.3",
    "sequelize": "^4.38.0",
    "truncate-utf8-bytes": "^1.0.2"
  }
}
